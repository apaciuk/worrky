<div class="page-header">
  <%= link_to jobs_path, class: 'btn btn-default' do %>
    All Jobs
  <% end %>
  <%= link_to edit_job_path(@job), class: 'btn btn-primary' do %>
    Edit
  <% end %>
  <h1>Show job</h1>
</div>

<dl class="dl-horizontal">
  <dt>Title:</dt>
  <dd><%= @job.title %></dd>
  <dt>Description:</dt>
  <dd><%= @job.description %></dd>
  <dt>Company:</dt>
  <dd><%= @job.company %></dd>
  <dt>Url:</dt>
  <dd><%= @job.url %></dd>
</dl>
